    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.1",
    "envalid": "^8.1.0",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-handlebars": "^8.0.3",
    "express-session": "^1.18.2",
    "joi": "^18.0.1",
    "joi-translation-pt-br": "^0.0.10",
    "lorem-ipsum": "^2.0.8",
    "sass": "^1.91.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
